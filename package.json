{
  "name": "react-mapbox-wrapper",
  "version": "0.0.1",
  "description": "Mapbox GL Javascript wrapper for ReactJS",
  "repository": "https://github.com/MeilleursAgents/react-mapbox-wrapper",
  "main": "build/index.js",
  "scripts": {
    "format": "./node_modules/.bin/prettier-eslint --write",
    "lint": "./node_modules/.bin/eslint --fix --ext .jsx,.js src",
    "test": "NODE_PATH=./src ./node_modules/.bin/nyc --all ./node_modules/.bin/mocha --opts ./mocha.opts \"./src/**/*.test.js\" \"./src/**/*.test.jsx\"",
    "test:file": "NODE_PATH=./src ./node_modules/.bin/mocha --opts ./mocha.opts",
    "doc": "./node_modules/.bin/esdoc -c esdoc.json",
    "start": "./node_modules/.bin/webpack --watch",
    "build": "./node_modules/.bin/webpack -p",
    "quality": "npm run format && npm run lint && npm run test && npm run doc && npm run build"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "mapboxgl-js",
    "webpack"
  ],
  "author": "MeilleursAgents",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@turf/circle": "6.0.1",
    "mapbox-gl": "0.46.0",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-template": "6.26.0",
    "babel-types": "6.26.0",
    "chai": "4.1.2",
    "enzyme": "3.5.0",
    "enzyme-adapter-react-16": "1.1.1",
    "esdoc": "1.1.0",
    "esdoc-coverage-plugin": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-es7-plugin": "0.0.3",
    "esdoc-jsx-plugin": "1.0.0",
    "esdoc-plugin-require-coverage": "0.1.2",
    "esdoc-publish-html-plugin": "1.1.2",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-loader": "2.1.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-react": "7.10.0",
    "jsdom": "12.0.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "prettier-eslint-cli": "4.7.1",
    "sinon": "5.1.1",
    "sinon-chai": "3.1.0",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.3"
  }
}
